{
  "version": 2,
  "builds": [
    {
      "src": "api/**.js",
      "use": "@vercel/node"
    }
  ],
  "env": {
    "ENDPOINT_URL": "@endpoint-url",
    "DEPLOYMENT_NAME": "@deployment-name",
    "SEARCH_ENDPOINT": "@search-endpoint",
    "SEARCH_KEY": "@search-key",
    "SEARCH_INDEX_NAME": "@search-index-name",
    "AZURE_OPENAI_API_KEY": "@azure-openai-api-key"
  }
}
